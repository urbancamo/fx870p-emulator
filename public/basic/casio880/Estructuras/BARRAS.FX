5 CLEAR:DIM F(5),A(5)
10 INPUT "N DE FUERZAS CONOCIDAS";N
20 H=0:V=0
30 FOR I=1 TO N
40 PRINT "F(";I;")=";:INPUT F(I)
50 PRINT "ANG(";I;")=";:INPUT A(I)
60 H=H+F(I)*COSA(I):V=V+F(I)*SINA(I)
70 NEXT I
80 PRINT "BARRAS DESCONOCIDAS"
90 INPUT "ANG1=";A1,"ANG2=";A2
100 D=COSA1*SINA2-SINA1*COSA2
110 N1=(V*COSA2-H*SINA2)/D
120 N2=(H*SINA1-V*COSA1)/D
130 PRINT "N1=";N1,"N2=";N2:GOTO 10
