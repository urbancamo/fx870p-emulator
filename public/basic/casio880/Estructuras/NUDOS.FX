10 PRINT "NUDOS":ERASE F,A
15 ANGLE 0
20 DIM F(5),A(5)
30 INPUT "N. FUERZAS CONOCIDAS";N
35 H=0:V=0
40 FOR I=1 TO N
50 PRINT "F(";I;")=";:INPUT F(I)
60 INPUT "ANGULO"; A(I)
70 H=H+F(I)*COSA(I):V=V+F(I)*SINA(I)
80 NEXT I
90 PRINT"BARRAS DESCONOCIDAS"
100 INPUT "ANG.1";A1,"ANG.2";A2
110 D1=COSA1*SINA2-SINA1*COSA2
115 DX=-H*SINA2+V*COSA2
120 DY=-V*COSA1+H*SINA1
130 N1=DX/D1:N2=DY/D1
140 PRINT "N1=";N1,"N2=";N2
150 INPUT "OTRO NUDO 1/A";Z:IF Z=1 THEN 30
160 END
