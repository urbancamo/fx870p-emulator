10 CLS:PRINT "CAMBIO ESTR/TRIANG"
20 INPUT "R1",R1
30 INPUT "R2",R2
40 INPUT "R3",R3
50 INPUT "1 ESTRELLA/TRIANGULO; 2 TRIANGULO ESTRELLA",A
60 ON A GOTO 70,90
70 R12=R1+R2+((R1*R2)/R3):R13=R1+R3+((R1*R3)/R2):R23=R2+R3+((R2*R3)/R1)
80 PRINT "R12=";ROUND(R12,-3);"R13=";ROUND(R13,-3);"R23=";ROUND(R23,-3):END
90 G1=1/R1:G2=1/R2:G3=1/R3
100 G13=G1+G3+((G1*G3)/G2):G23=G2+G3+((G2*G3)/G1):G12=G1+G2+((G1*G2)/G3)
110 PRINT "R13=";ROUND(1/G13,-3);"R23=";ROUND(1/G23,-3);"R12=";ROUND(1/G12,-3):END

